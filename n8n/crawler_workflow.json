{
    "name": "Young & Home - Super Crawler Agent",
    "nodes": [
        {
            "parameters": {
                "rule": {
                    "interval": [
                        {
                            "field": "days",
                            "daysInterval": 1
                        }
                    ]
                }
            },
            "id": "schedule-trigger",
            "name": "매일 실행",
            "type": "n8n-nodes-base.scheduleTrigger",
            "typeVersion": 1.1,
            "position": [
                250,
                300
            ]
        },
        {
            "parameters": {
                "url": "http://api:8000/api/agent/crawler/run",
                "method": "POST",
                "options": {}
            },
            "id": "call-super-agent",
            "name": "Super Agent 실행 (공고 수집)",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 4.1,
            "position": [
                450,
                300
            ],
            "notes": "복잡한 로직 없이 Python Super Agent 호출만 수행"
        }
    ],
    "connections": {
        "schedule-trigger": {
            "main": [
                [
                    {
                        "node": "call-super-agent",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        }
    },
    "settings": {
        "executionOrder": "v1"
    },
    "meta": {
        "templateId": "young-and-home-super-crawler"
    }
}